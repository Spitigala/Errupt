<div class="dashboard-content small-12 large-9 columns">
	<div class="row">
		<div class="dash-err-title small-12 large-6 columns">
			<h3><i class="fa fa-list-ol"></i> Your Error Logs</h3>
		</div>
		<div class="dash-overview-title small-5 columns desktop-only overview-title">
			<h3><i class="fa fa-pie-chart"></i> Overview</h3>
		</div>
	</div>

	<div class="row">
		<div class="panel small-12 large-6 columns container1">
			<%= render 'errorlog_list_widget' %>
		</div>

		<div class="panel large-6 columns container2 desktop-only">
			<%= render 'overview_widget' %>
		</div>
		
		<%= render 'taglist_widget' %>

		<%= render 'errorlog_piechart_widget' %>
	</div>
</div>


